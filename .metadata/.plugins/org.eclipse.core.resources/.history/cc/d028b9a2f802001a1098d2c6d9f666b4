package ObserverPattern;

public class DogBot {

	protected int hungry, tired;
	private String name;
	
	public DogBot(String nm, int h, int t){
		name=nm;
		hungry=h;
		tired=t;
	}
	
	public boolean eat(){
		if(hungry>6){
			hungry-=3;
			return true;
		}else{
			return false;
		}
	}
	
	public void rest(){
		hungry++;
		tired-=2;
	}
	public void play(){
		hungry+=2;
		tired+=3;
	}
	public String getName(){
		return name;
	}
	
	
}
